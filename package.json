{
  "name": "tslib",
  "version": "1.0.0",
  "description": "ts library",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive --require ts-node/register \"test/**/*.ts\"",
    "cover": "nyc npm run test"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mebius1080p/TSLib.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mebius1080p/TSLib/issues"
  },
  "homepage": "https://github.com/mebius1080p/TSLib#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "nyc": {
    "include": [
      "src/ts/*.ts",
      "src/ts/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
