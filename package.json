{
  "name": "tslib",
  "version": "1.0.0",
  "description": "ts library",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive --require ts-node/register \"test/**/*.ts\"",
    "cover": "nyc npm run test"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mebius1080p/TSLib.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mebius1080p/TSLib/issues"
  },
  "homepage": "https://github.com/mebius1080p/TSLib#readme",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "ts-loader": "^5.2.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.2"
  },
  "nyc": {
    "include": [
      "src/ts/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "src/**/*.d.ts"
